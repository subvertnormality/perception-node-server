<canvas class="static" id="static"></canvas>
<canvas class="cozmoStream" id="cozmoStream" width="100%", height="100%"></canvas>
<div class="hud">
  <img src="assets/logo.gif" alt="perception" /><br/>
  <div id="queueStatus"><span class="consoleText">Initialising....<span></div>
  <br/><div id="commandPrompt"></div>
</div>
<div class="user" id="userHud">
</div>
<div id="controls">
  <div class="controlsLeft">
    <div class="keysTopRow">
      <icon class="keyboard-key return dark"></icon>
      <icon class="keyboard-key r dark"></icon>
      <icon class="keyboard-key t dark"></icon>
    </div>
    <div class="keysBottomRow">
      <icon class="keyboard-key shift dark"></icon>
      <icon class="keyboard-key f dark"></icon>
      <icon class="keyboard-key g dark"></icon>

    </div>
  </div>

  <div class="controlsRight">
    <div class="keysTopRow">
      <icon class="keyboard-key w dark"></icon>
    </div>
    <div class="keysBottomRow">
      <icon class="keyboard-key a dark"></icon>
      <icon class="keyboard-key s dark"></icon>
      <icon class="keyboard-key d dark"></icon>

    </div>
  </div>
</div>