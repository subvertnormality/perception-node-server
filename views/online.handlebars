<canvas class="static" id="static"></canvas>
<canvas class="cozmoStream" id="cozmoStream" width="100%", height="100%"></canvas>
<div class="perceptionStream" id="perceptionStream"></div>
<div class="hud">
  <span id="logo" class="logoContainer"><img class="logo" src="assets/logo.svg" alt="perception" /><br/></span>
  <div id="queueStatus"><span class="consoleText">Initialising....<span></div>
  <br/>  
  <div class="commandContainer" id="commandContainer">
    <input type="text" class="commandPrompt" id="commandPrompt" size="1"></input>
  </div>
  <div class="console" id="console"></div>
</div>
<div class="user" id="userHud">
</div>
<div id="controls">
  <div class="controlsLeft">
    <div class="keysTopRow">
      <button class="keyboard-key w dark" id="wKey"></button>
    </div>
    <div class="keysBottomRow">
      <button class="keyboard-key a dark" id="aKey"></button>
      <button class="keyboard-key s dark" id="sKey"></button>
      <button class="keyboard-key d dark" id="dKey"></button>
    </div>
  </div>
  <div class="controlsRight">
    <div class="keysTopRow">
      <button class="keyboard-key r dark" id="rKey"></button>
      <button class="keyboard-key t dark" id="tKey"></button>
    </div>
    <div class="keysBottomRow">
      <button class="keyboard-key return dark" id="returnKey"></button>
      <button class="keyboard-key f dark" id="fKey"></button>
      <button class="keyboard-key g dark" id="gKey"></button>
    </div>
  </div>
</div>
<script src= "https://player.twitch.tv/js/embed/v1.js"></script>  