<script>
var addr = "{{addr}}";
var sechash = "{{sechash}}";
</script>
<div class="level text"><span class="consoleText">LEVEL: {{level}}</span></div>
{{#if roundComplete}}
  <div class="blocks">
    <div id="one" class="block one {{oneClasses}}"></div>
    <div id="two" class="block two {{twoClasses}}"></div>
    <div id="three" class="block three {{threeClasses}}"></div>
    <div id="four" class="block four {{fourClasses}}"></div>
    <div id="five" class="block five {{fiveClasses}}"></div>
    <div id="six" class="block six {{sixClasses}}"></div>
  </div>
{{else}}
  <div class="roundHint text"><span class="consoleText">SAY THE WORD:</span></div>
  <div class="clue text"><span class="consoleText">{{clue}}</span></div>
  <img src="{{secureAddr}}game/{{imageOne}}" id="imageOne" style="display: none;" />
  <img src="{{secureAddr}}game/{{imageTwo}}" id="imageTwo" style="display: none;" />
  <img src="{{secureAddr}}game/{{imageThree}}" id="imageThree" style="display: none;" />
  <canvas class="static" id="static"></canvas>
{{/if}}
<script src='https://cdn.socket.io/socket.io-1.4.5.js'></script>
<script src="{{addr}}javascript/static.js"></script>
<script src="{{addr}}javascript/ui.js"></script>